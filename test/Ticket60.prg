#include "hwguipp.ch"

***********************
FUNCTION Main()
***********************
LOCAL oMainW , oFontMain , cImageDir , oIcon, cImgok , cImgExcl
LOCAL oIExcl , cValok , cValExcl
PUBLIC cHexok, cHexExcl

#ifdef __GTK__
cImageDir := ".." + hwg_GetDirSep() + ".." + hwg_GetDirSep() + "image" + hwg_GetDirSep()
#else
cImageDir := ".." + hwg_GetDirSep() + "image" + hwg_GetDirSep()
#endif

* ---------------------------
* Ressources by hex values
* ---------------------------
Init_Hexvars()


cValok    := hwg_cHex2Bin ( cHexok )
cValExcl  := hwg_cHex2Bin ( cHexExcl )
oIcon     := HIcon():AddString( "ok"   , cValok   , 75 , 75 )
oIExcl    := HIcon():AddString( "Excl" , cValExcl , 8  , 8  )
 
 
* --------------------------- 
* Load resources from files
* ---------------------------

/*
