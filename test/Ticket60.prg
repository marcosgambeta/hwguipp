*/

#ifdef __PLATFORM__WINDOWS
   PREPARE FONT oFontMain NAME "MS Sans Serif" WIDTH 0 HEIGHT -14
#else
   PREPARE FONT oFontMain NAME "Sans" WIDTH 0 HEIGHT 12 
#endif


INIT WINDOW oMainW  ;
   FONT oFontMain  ;
   TITLE "Ticket #60 test sample" AT 0, 0 SIZE 300 , 200 ;
   ICON oIcon STYLE WS_POPUP +  WS_CAPTION + WS_SYSMENU ;
   BACKCOLOR  0x3C3940   
   
   @ 10 , 10 ICON oIcon   && SIZE 75 , 75
   
   @ 10, 100 ICON oIExcl  && SIZE 8 , 8
   
   oMainW:Activate()   

RETURN NIL

